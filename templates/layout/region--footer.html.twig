<script>
page.loader(false);

la.cannot_patient = bodymovin.loadAnimation(setLottieOptions({
  loop: false,
  autoplay: false,
  key: 'cannot_patient',
  name: 'cannot_patient'
}));

la.cannot_patient_desktop = bodymovin.loadAnimation(setLottieOptions({
  loop: true,
  autoplay: false,
  key: 'cannot_patient_desktop',
