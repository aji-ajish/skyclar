  setTimeout(function () {
	page.loader(false);
	lottieAnimate({
	  nitroTrigger: {
		key: 'cannot_patient'
	  }
	});
	lottieAnimate({
	  nitroTrigger: {
		key: 'cannot_patient_desktop'
	  }
	});

  }, 1000);
});
</script>